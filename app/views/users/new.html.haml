.bike-background
  .inner
    .session-wrapper
      %h2
        Sign Up
      = form_for @user do |form| 
        = render partial: '/users/form', object: form
        .submit-field
          = form.submit "Sign Up", class: "btn btn-aqua"
        .other-links
          = link_to "Sign In", sign_in_path, class: "btn btn-dark-aqua"

